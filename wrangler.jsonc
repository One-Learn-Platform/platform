{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "platform",
	"compatibility_date": "2025-06-05",
	"compatibility_flags": ["nodejs_compat"],
	"main": ".svelte-kit/cloudflare/_worker.js",
	"preview_urls": true,
	"observability": {
		"enabled": true,
	},
	"assets": {
		"binding": "ASSETS",
		"directory": ".svelte-kit/cloudflare",
	},
	"vars": {
		"NODE_VERSION": "22.14.0",
		"PNPM_VERSION": "10.11.0",
		"PUBLIC_TURNSTILE_SITE_KEY": "0x4AAAAAAA5h5cmpBN6IvbtJ",
		"PUBLIC_R2_URL": "https://media.onelearn.id",
	},
	"env": {
		"production": {
			"d1_databases": [
				{
					"binding": "DB",
					"database_id": "8d67040c-2232-4ea0-9a03-54d674b839a9",
					"database_name": "prod-db",
					"migrations_dir": "./src/lib/server/db/migrations",
				},
			],
			"r2_buckets": [
				{
					"binding": "MEDIA",
					"bucket_name": "media",
				},
			],
		},
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_id": "8d67040c-2232-4ea0-9a03-54d674b839a9",
			"database_name": "prod-db",
			"migrations_dir": "./src/lib/server/db/migrations",
		},
	],
	"r2_buckets": [
		{
			"binding": "MEDIA",
			"bucket_name": "media",
		},
	],
}
